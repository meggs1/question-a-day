<h1>Log in page</h1>
<%= form_with(model: @user, url: "/login") do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br>
    <br>
<%= f.submit "Login" %>
<%= button_to "Log in with Google", '/auth/google_oauth2'  %>
<%end%>